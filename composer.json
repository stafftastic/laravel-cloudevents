{
    "name": "stafftastic/laravel-cloudevents",
    "description": "A reusable cloudevent system for laravel projects.",
    "type": "library",
    "license": "Commercial",
    "authors": [
        {
            "name": "Hendrik Heil",
            "email": "hendrik@stafftastic.de"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
        "cloudevents/sdk-php": "^1.0",
        "mateusjunges/laravel-kafka": "^1.8"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.1",
        "psalm/plugin-laravel": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "stafftastic\\CloudEvents\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "stafftastic\\CloudEvents\\CloudEventsServiceProvider"
            ]
        }
    }
}
